<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.java.micarro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.java.micarro.model.MyFirebaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="miAuto"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.java.micarro.MenuLateralActivity"
            android:label="@string/title_activity_menu_lateral"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.java.micarro.IngresarPersona" />
        <activity android:name="com.java.micarro.IngresarAuto" />
        <activity android:name="com.java.micarro.Principal" />
        <activity
            android:name="com.java.micarro.MainActivity"
            android:label="miAuto">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>